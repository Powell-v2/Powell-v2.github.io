{
  "name": "Dream.Code.Sleep.Repeat.",
  "private": true,
  "description": "Personal website&blog of Powell Ye",
  "version": "3.2.0",
  "author": "Pavel Yermolin <oatto.paul@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "npm run build && npx gh-pages -d public -b master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.14",
    "@emotion/styled": "10.0.14",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-brands-svg-icons": "5.9.0",
    "@fortawesome/free-regular-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@mdx-js/mdx": "1.0.21",
    "@mdx-js/react": "1.0.21",
    "gatsby": "2.9.8",
    "gatsby-image": "2.2.4",
    "gatsby-mdx": "0.6.3",
    "gatsby-plugin-emotion": "4.1.0",
    "gatsby-plugin-manifest": "2.2.1",
    "gatsby-plugin-offline": "2.2.1",
    "gatsby-plugin-react-helmet": "3.1.0",
    "gatsby-plugin-sharp": "2.2.3",
    "gatsby-remark-images": "3.0.16",
    "gatsby-source-filesystem": "2.1.2",
    "gatsby-transformer-sharp": "2.2.1",
    "gsap": "2.1.3",
    "normalize.css": "8.0.1",
    "prism-react-renderer": "0.1.7",
    "prop-types": "^15.6.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-react-hooks": "1.6.1",
    "gatsby-cli": "2.7.8",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5"
  },
  "keywords": [
    "gatsby",
    "blog",
    "personal-website"
  ]
}
