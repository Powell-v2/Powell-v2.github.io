{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A website built with Gatsby",
  "version": "0.1.0",
  "author": "Pavel Ermolin <oatto.paul@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "deploy": "gatsby build && npx gh-pages -d public -b master"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npx eslint --fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "@fortawesome/fontawesome-svg-core": "1.2.15",
    "@fortawesome/free-brands-svg-icons": "5.7.2",
    "@fortawesome/free-regular-svg-icons": "5.7.2",
    "@fortawesome/free-solid-svg-icons": "5.7.2",
    "@fortawesome/react-fontawesome": "0.1.4",
    "@mdx-js/mdx": "1.0.10",
    "@mdx-js/react": "1.0.6",
    "gatsby": "2.1.8",
    "gatsby-image": "^2.0.20",
    "gatsby-mdx": "0.6.2",
    "gatsby-plugin-emotion": "4.0.4",
    "gatsby-plugin-manifest": "^2.0.15",
    "gatsby-plugin-offline": "^2.0.22",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.0.19",
    "gatsby-source-filesystem": "^2.0.8",
    "gatsby-transformer-sharp": "^2.1.13",
    "gsap": "2.0.2",
    "normalize.css": "8.0.1",
    "prop-types": "^15.6.2",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "1.6.0",
    "gh-pages": "2.0.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5"
  },
  "keywords": [
    "gatsby",
    "blog",
    "personal-website"
  ]
}
